# Database
DATABASE_URL=sqlite:///./voip_onboarding.db

# JWT Configuration
JWT_SECRET_KEY=your-super-secret-key-change-in-production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=1440

# FreePBX API Configuration
FREEPBX_HOST=https://your-freepbx-server.com
FREEPBX_API_TOKEN=your-freepbx-api-token
FREEPBX_API_USER=admin

# SIP Configuration (for client softphones)
SIP_DOMAIN=your-freepbx-server.com
SIP_PORT=5060
SIP_TRANSPORT=udp

# Extension numbering
EXTENSION_START=1000
